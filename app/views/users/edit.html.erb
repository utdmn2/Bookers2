<h2>User info</h2>
<% if @user.errors.any? %>
  <%= @user.errors.count %>error prohibited this obj from being saved:
   <% @user.errors.full_messages.each do |message| %>
   <%= message %>
  <% end %>
<% end %

<%= form_with model:@user, local:true do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName" %>


  <label for="inputImage">ProfileImage</label>
  <%= f.attachment_field :profile_image, placeholder: "プロフィール画像" %>

 <label for="inputIntroduction">Intoroduction</label>
  <%= f.text_field :introduction, autofocus: true, id:"inputIntroduction" %>
  <%= f.submit "Update User" %>
<% end %>