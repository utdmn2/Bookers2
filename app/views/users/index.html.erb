<%= render 'users/sidebar' %>
<h1>Users</h1>
<table>
 <thead>
  <tr>
   <th>image</th>
   <th>name</th>
   <th></th>
  </tr>
 </thead>

 <tbody>

    <% @users.each do |user| %>
 <tr>
   <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></td>
   <td><p><%= user.name %></p></td>
   <td><p><%= link_to "Show",user_path(user.id) %></p></td>
 </tr>
    <% end %>
 </tbody>
</table>